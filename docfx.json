{  
  "build": {
    "content": [      
      {
        "files": [          
          "tools/html/**/*.md",
		  "tools/html/**/toc.yml",
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [          	  
          "images/**"
        ]
      }
    ],    	
    "dest": "html",
	"globalMetadata": {
      "_appTitle": "Stack Overflow answers",
      "_appFooter": "<span>Stack Overflow answers (published from sources in <a href=\"https://github.com/MSDN-WhiteKnight/answers\">GitHub repository</a>). Copyright (c) 2020, MSDN.WhiteKnight. Content licensed under <a href=\"https://github.com/MSDN-WhiteKnight/answers/blob/master/LICENSE\">BSD 3-Clause License</a>.<br/>Generated by <strong>DocFX</strong></span>"      
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
